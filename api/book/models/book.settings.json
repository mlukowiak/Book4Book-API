{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "name": "Book"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "author": {
      "type": "string"
    },
    "publishingHouse": {
      "type": "string"
    },
    "series": {
      "type": "string"
    },
    "ads": {
      "collection": "ad",
      "via": "book"
    },
    "publishYear": {
      "type": "integer"
    },
    "types": {
      "collection": "type"
    }
  }
}
